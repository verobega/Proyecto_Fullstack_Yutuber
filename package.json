    "@types/bcryptjs": "^2.4.2",
    "@vercel/node": "^2.6.2",
    "bcryptjs": "^2.4.3",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "ytdl-core": "^4.11.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
